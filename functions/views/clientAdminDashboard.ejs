						<div class="time-">
							<div class="time-clock">
								<div class="ana-body">
									<div class="ana" role="progressbar" aria-valuenow="20" aria-valuemin="0"
										aria-valuemax="100" style="--value: calc(20 / 100 * 100); --final: 20"></div>
								</div>
							</div>
							<div class="time-clock-text">
								<div class="time-num" style="line-height: normal;">19560</div>
								<div class="time-num-text" style="font-size: 10px; position: relative; bottom: 8px">Minutes</div>
							</div>
						</div>
						<div class="waves-effect waves-light cbtn upgd" style="font-size: 14px;">Upgrade</div>
					</div>
				</div>
			</div>
		</div>
